EXPLANATIONS: <br>

<% @lessonExp.each do |lex| %>
	<% @explanation = Explanation.where(id: lex.explanation_id) %>
	<% if @explanation %>
	<% else %>
		<%= lex.id %> - lesson: <%= lex.lesson_id %> - explanation: <%= lex.explanation_id %> | <br>
	<% end %>
<% end %>

