<% @course_item = @new_course %>
var newCourse = "<%= escape_javascript (render partial: './courses/card', course: @course_item ) %>";
$('.card_list.creator_list').prepend(newCourse);

var newHomeCard = $('.card_<%= @course_item.id %>');
// console.log(newHomeCard);
newHomeCard.addClass('newcard');

setTimeout(function(){
    $('.js-create_course_form').fadeOut(500);
}, 500);
setTimeout(function(){
	newHomeCard.removeClass('newcard');
}, 3000);