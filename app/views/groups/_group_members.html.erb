<div class="shadebox">
  <div class="shade box">
    <div class="shadebox_title">
      <%= @group.name %>
    </div>
    <div class="shadebox_content" style="text-align:center;">
      <%@users_in_group.each do |user| %>
        <%= user.first_name %> <%= user.last_name %>
      <% end %>
      <% @users_in_group.each_slice(6) do |slice| %>
          <div class="user_column">
            <% slice.each do |image| -%>
                <%= image_tag(image.url, :alt => image.alt) %>
            <% end -%>
          </div>
      <% end -%>
      <hr>
    </div>
    <div class="shadebox_bottom">
      <button type="button" class="mark_as_completed green_sft" style="float:left;" data-rehearsalid="" data-userid="<%= current_user.id %>">Mark as completed</button>
      <a href="" class="leave_feedback">
        <button type="button" class="blue leave_feedback" style="float:left;" data-rehearsalid=" ">Leave Video Feedback</button>
      </a>
    </div>
  </div>
</div>'