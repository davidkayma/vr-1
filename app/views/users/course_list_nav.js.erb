'use strict';

$('.admin_c_list.card_list').empty();
<% @courses.each do |course| %>

    var card = "<%= escape_javascript (render partial: './courses/card', locals: { course_item: course } ) %>";
    $('.admin_c_list.card_list').append(card);

<% end %>

$('.current_nav').val( <%= @c %> );
$('.current_nav_num').text( <%= @c + 1 %> );