<br>
<% t = [] %>
<% @course.topics.each do |topic| %>
  <% topic.lessons.each do |lesson| %>
    <% lesson.rehearsals.each do |rehearsal| %>
      <% if rehearsal.submission == true %>
        <% t << topic %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
<% t.uniq.each do |t| %>
  <%= link_to t.title, course_topic_path(@course, t) %> <br>
<% end %>
